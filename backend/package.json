{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prettier": "@openmfp/config-prettier",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --passWithNoTests"
  },
  "dependencies": {
    "@kubernetes/client-node": "1.4.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/serve-static": "^5.0.4",
    "@openmfp/portal-server-lib": "0.163.4",
    "axios": "^1.6.3",
    "class-validator": "0.14.3",
    "cookie-parser": "1.4.7",
    "dotenv": "17.2.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@openmfp/config-prettier": "0.9.2",
    "@openmfp/eslint-config-typescript": "0.7.7",
    "@types/express": "5.0.6",
    "@types/jest": "^29.5.2",
    "@types/node": "24.10.1",
    "@types/supertest": "6.0.3",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "eslint": "9.39.2",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.7",
    "nock": "14.0.10",
    "prettier": "3.7.4",
    "source-map-support": "0.5.21",
    "supertest": "7.1.4",
    "ts-jest": "^29.1.0",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "passWithNoTests": true,
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^@openmfp/portal-lib(|/.*)$": "<rootDir>/libs/portal-lib/src/$1"
    }
  },
  "type": "module"
}
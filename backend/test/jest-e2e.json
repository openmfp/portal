{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "openmfp/portal-lib/(.*)": "<rootDir>/../libs/portal-lib/src/$1",
    "openmfp/portal-lib": "<rootDir>/../libs/portal-lib/src"
  }
}
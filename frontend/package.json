    "@angular/build": "^21.0.0",
    "@angular/cli": "^21.0.0",
    "@angular/compiler-cli": "^21.0.0",
    "@briebug/jest-schematic": "6.0.0",
    "@openmfp/config-prettier": "0.9.2",
    "@openmfp/eslint-config-typescript": "0.7.7",
    "@types/jest": "^30.0.0",
    "jest": "^30.0.0",
    "nodemon": "3.1.14",
    "rimraf": "6.1.3",
    "typescript": "~5.8.0"
  }

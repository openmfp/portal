{
  "name": "frontend",
  "version": "0.0.0",
  "prettier": "@openmfp/config-prettier",
  "scripts": {
    "ng": "ng",
    "extract-versions": "node ./build-scripts/extract-versions.js",
    "prebuild": "npm run extract-versions",
    "prebuild:watch": "npm run extract-versions",
    "prestart": "npm run extract-versions",
    "prestart:watch": "npm run extract-versions",
    "start": "ng serve --port 4300",
    "start:watch": "nodemon --watch .yalc -e * --exec \"rimraf .angular && ng serve --watch --port 4300\"",
    "build": "ng build",
    "build:watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/jest": "21.0.3",
    "@angular/animations": "^21.2.0",
    "@angular/cdk": "^21.2.0",
    "@angular/common": "^21.2.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/elements": "^21.2.0",
    "@angular/forms": "^21.2.0",
    "@angular/localize": "^21.2.0",
    "@angular/platform-browser": "^21.2.0",
    "@angular/platform-browser-dynamic": "^21.2.0",
    "@angular/router": "^21.2.0",
    "@luigi-project/client": "^2.25.1",
    "@luigi-project/core": "^2.25.1",
    "@luigi-project/plugin-auth-oauth2": "^2.25.1",
    "@openmfp/portal-ui-lib": "^0.186.15",
    "@ui5/webcomponents-ngx": "^0.5.10",
    "jmespath": "^0.16.0",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.23",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1"
  },
